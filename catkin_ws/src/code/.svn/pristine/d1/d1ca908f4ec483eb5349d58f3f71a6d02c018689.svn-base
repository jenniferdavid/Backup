<div class="line"><a name="l00060"></a><span class="lineno">   60</span>&#160;{</div>
<div class="line"><a name="l00061"></a><span class="lineno">   61</span>&#160;  <a class="code" href="cell_8h.html#structestar__cell__t">estar_cell_t</a> ** prop;</div>
<div class="line"><a name="l00062"></a><span class="lineno">   62</span>&#160;  <a class="code" href="cell_8h.html#structestar__cell__t">estar_cell_t</a> * primary;</div>
<div class="line"><a name="l00063"></a><span class="lineno">   63</span>&#160;  <a class="code" href="cell_8h.html#structestar__cell__t">estar_cell_t</a> * secondary;</div>
<div class="line"><a name="l00064"></a><span class="lineno">   64</span>&#160;  <span class="keywordtype">double</span> rr;</div>
<div class="line"><a name="l00065"></a><span class="lineno">   65</span>&#160;  </div>
<div class="line"><a name="l00066"></a><span class="lineno">   66</span>&#160;  cell-&gt;<a class="code" href="cell_8h.html#a96ac8b6ae47a8f10ecac3f7107058409">rhs</a> = INFINITY;</div>
<div class="line"><a name="l00067"></a><span class="lineno">   67</span>&#160;  prop = cell-&gt;<a class="code" href="cell_8h.html#ad379ae7988322232d9821c72dbbd678a">prop</a>;</div>
<div class="line"><a name="l00068"></a><span class="lineno">   68</span>&#160;  <span class="keywordflow">while</span> (NULL != *prop) {</div>
<div class="line"><a name="l00069"></a><span class="lineno">   69</span>&#160;    </div>
<div class="line"><a name="l00070"></a><span class="lineno">   70</span>&#160;    primary = *(prop++);</div>
<div class="line"><a name="l00071"></a><span class="lineno">   71</span>&#160;    <span class="keywordflow">if</span> (primary-&gt;<a class="code" href="cell_8h.html#a96ac8b6ae47a8f10ecac3f7107058409">rhs</a> &lt;= (*prop)-&gt;rhs)  {</div>
<div class="line"><a name="l00072"></a><span class="lineno">   72</span>&#160;      secondary = *(prop++);</div>
<div class="line"><a name="l00073"></a><span class="lineno">   73</span>&#160;    }</div>
<div class="line"><a name="l00074"></a><span class="lineno">   74</span>&#160;    <span class="keywordflow">else</span> {</div>
<div class="line"><a name="l00075"></a><span class="lineno">   75</span>&#160;      secondary = primary;</div>
<div class="line"><a name="l00076"></a><span class="lineno">   76</span>&#160;      primary = *(prop++);</div>
<div class="line"><a name="l00077"></a><span class="lineno">   77</span>&#160;    }</div>
<div class="line"><a name="l00078"></a><span class="lineno">   78</span>&#160;    </div>
<div class="line"><a name="l00079"></a><span class="lineno">   79</span>&#160;    <span class="comment">// do not propagate from obstacles, queued cells, cells above the</span></div>
<div class="line"><a name="l00080"></a><span class="lineno">   80</span>&#160;    <span class="comment">// wavefront, or cells at infinity</span></div>
<div class="line"><a name="l00081"></a><span class="lineno">   81</span>&#160;    <span class="keywordflow">if</span> (primary-&gt;<a class="code" href="cell_8h.html#aff00ddf90f9ec54b1655ccaa5c269a22">flags</a> &amp; <a class="code" href="cell_8h.html#a06fc87d81c62e9abb8790b6e5713c55bae5c16afc48725de7df409e12a8478c3b">ESTAR_FLAG_OBSTACLE</a></div>
<div class="line"><a name="l00082"></a><span class="lineno">   82</span>&#160;        || primary-&gt;<a class="code" href="cell_8h.html#a1d833386d3e37e42b68cc008bfdd6991">pqi</a> != 0</div>
<div class="line"><a name="l00083"></a><span class="lineno">   83</span>&#160;        || primary-&gt;<a class="code" href="cell_8h.html#a5af06652dbd9b071bee86234a1e02ca7">phi</a> &gt; phimax</div>
<div class="line"><a name="l00084"></a><span class="lineno">   84</span>&#160;        || isinf(primary-&gt;<a class="code" href="cell_8h.html#a5af06652dbd9b071bee86234a1e02ca7">phi</a>)) {</div>
<div class="line"><a name="l00085"></a><span class="lineno">   85</span>&#160;      <span class="keywordflow">continue</span>;</div>
<div class="line"><a name="l00086"></a><span class="lineno">   86</span>&#160;    }</div>
<div class="line"><a name="l00087"></a><span class="lineno">   87</span>&#160;    </div>
<div class="line"><a name="l00088"></a><span class="lineno">   88</span>&#160;    <span class="comment">// the same goes from the secondary, but if that fails at least we</span></div>
<div class="line"><a name="l00089"></a><span class="lineno">   89</span>&#160;    <span class="comment">// can fall back to the non-interpolated update equation.</span></div>
<div class="line"><a name="l00090"></a><span class="lineno">   90</span>&#160;    <span class="keywordflow">if</span> (secondary-&gt;<a class="code" href="cell_8h.html#aff00ddf90f9ec54b1655ccaa5c269a22">flags</a> &amp; <a class="code" href="cell_8h.html#a06fc87d81c62e9abb8790b6e5713c55bae5c16afc48725de7df409e12a8478c3b">ESTAR_FLAG_OBSTACLE</a></div>
<div class="line"><a name="l00091"></a><span class="lineno">   91</span>&#160;        || secondary-&gt;<a class="code" href="cell_8h.html#a1d833386d3e37e42b68cc008bfdd6991">pqi</a> != 0</div>
<div class="line"><a name="l00092"></a><span class="lineno">   92</span>&#160;        || secondary-&gt;<a class="code" href="cell_8h.html#a5af06652dbd9b071bee86234a1e02ca7">phi</a> &gt; phimax</div>
<div class="line"><a name="l00093"></a><span class="lineno">   93</span>&#160;        || isinf(secondary-&gt;<a class="code" href="cell_8h.html#a5af06652dbd9b071bee86234a1e02ca7">phi</a>)) {</div>
<div class="line"><a name="l00094"></a><span class="lineno">   94</span>&#160;      rr = primary-&gt;<a class="code" href="cell_8h.html#a96ac8b6ae47a8f10ecac3f7107058409">rhs</a> + cell-&gt;<a class="code" href="cell_8h.html#aa1a3428f3d4b95255d75bba00329fee2">cost</a>;</div>
<div class="line"><a name="l00095"></a><span class="lineno">   95</span>&#160;    }</div>
<div class="line"><a name="l00096"></a><span class="lineno">   96</span>&#160;    <span class="keywordflow">else</span> {</div>
<div class="line"><a name="l00097"></a><span class="lineno">   97</span>&#160;      rr = <a class="code" href="estar_8c.html#a17fbb639f85fb9420e03bce6ef59930d">interpolate</a> (cell-&gt;<a class="code" href="cell_8h.html#aa1a3428f3d4b95255d75bba00329fee2">cost</a>, primary-&gt;<a class="code" href="cell_8h.html#a5af06652dbd9b071bee86234a1e02ca7">phi</a>, secondary-&gt;<a class="code" href="cell_8h.html#a5af06652dbd9b071bee86234a1e02ca7">phi</a>);</div>
<div class="line"><a name="l00098"></a><span class="lineno">   98</span>&#160;    }</div>
<div class="line"><a name="l00099"></a><span class="lineno">   99</span>&#160;    </div>
<div class="line"><a name="l00100"></a><span class="lineno">  100</span>&#160;    <span class="keywordflow">if</span> (rr &lt; cell-&gt;rhs) {</div>
<div class="line"><a name="l00101"></a><span class="lineno">  101</span>&#160;      cell-&gt;<a class="code" href="cell_8h.html#a96ac8b6ae47a8f10ecac3f7107058409">rhs</a> = rr;</div>
<div class="line"><a name="l00102"></a><span class="lineno">  102</span>&#160;    }</div>
<div class="line"><a name="l00103"></a><span class="lineno">  103</span>&#160;  }</div>
<div class="line"><a name="l00104"></a><span class="lineno">  104</span>&#160;  </div>
<div class="line"><a name="l00105"></a><span class="lineno">  105</span>&#160;  <span class="keywordflow">if</span> (isinf (cell-&gt;<a class="code" href="cell_8h.html#a96ac8b6ae47a8f10ecac3f7107058409">rhs</a>)) {</div>
<div class="line"><a name="l00106"></a><span class="lineno">  106</span>&#160;    <span class="comment">// None of the above worked, we&#39;re probably done... but I have</span></div>
<div class="line"><a name="l00107"></a><span class="lineno">  107</span>&#160;    <span class="comment">// lingering doubts about about the effects of in-place primary /</span></div>
<div class="line"><a name="l00108"></a><span class="lineno">  108</span>&#160;    <span class="comment">// secondary sorting above, it could be imagined to create</span></div>
<div class="line"><a name="l00109"></a><span class="lineno">  109</span>&#160;    <span class="comment">// situations where we overlook something. So, just to be on the</span></div>
<div class="line"><a name="l00110"></a><span class="lineno">  110</span>&#160;    <span class="comment">// safe side, let&#39;s retry all non-interpolated options.</span></div>
